{
  "name": "web-to-figma-plugin",
  "version": "1.0.0",
  "description": "Import web pages as editable Figma designs",
  "scripts": {
    "build": "tsc && tsc -p tsconfig.ui.json",
    "watch": "concurrently \"tsc --watch\" \"tsc -p tsconfig.ui.json --watch\""
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.90.0",
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.0",
    "typescript": "^5.3.0"
  }
}
